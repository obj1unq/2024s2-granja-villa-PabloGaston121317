import cultivos.*
import hector.*
import wollok.game.*

describe "granjaVilla" {

  method initialize() {
    game.height(10)
	  game.width(10)

    const maiz = new Maiz(position= hector.position())
  }
  test "hector planta un maiz en su posicion actual" {
    const maiz = new Maiz(position= game.at(3,3))
    hector.position(game.at(3,3))
    hector.sembrarMaiz()

    assert.equals(game.at(3,3), maiz.position() )


  }

}